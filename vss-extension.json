{
  "manifestVersion": 1,
  "id": "devops-ext-better-checklist",
  "publisher": "Grollmus GmbH",
  "version": "0.1.6",
  "name": "Better checklist extension",
  "description": "Devops add-on for creating and managing checklists",
  "public": false,
  "categories": [
    "Azure Repos"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "scopes": [
    "vso.work_write"
  ],
  "contributions": [
    {
      "id": "devops-ext-better-checklist",
      "type": "ms.vss-web.hub",
      "targets": [
        "ms.vss-work-web.work-hub-group"
      ],
      "properties": {
        "name": "Better checklist",
        "uri": "dist/index.html"
      }
    }
  ],
  "files": [
    {
      "path": "dist",
      "addressable": true
    }
  ]
}
